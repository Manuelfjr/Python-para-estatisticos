    "import numpy as np\n",
    "\n",
    "def generate_data(n = 3, p = 2,f = lambda x: 2 * x[:,0] + 3*x[:,1] + 1):\n",
    "    \n",
    "    x = np.random.random((n, p))\n",
    "    \n",
    "    y = f(x) + np.random.normal(0, 1, n)\n",
    "    \n",
    "    return x,y\n",
    "X,Y = generate_data()"
   ]
